{
  "name": "@tensor-hq/tcomp-ts",
  "version": "0.11.2",
  "description": "Anchor/JS SDK for interacting with TCOMP.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "repository": "https://github.com/tensor-hq/tcomp.git",
  "license": "MIT",
  "publishConfig": {
    "@tensor-hq:registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "anchor build && bash scripts/cp_idl.sh",
    "build:test": "bash scripts/test_build.sh",
    "build:ts": "rm -rf ./dist && yarn tsc && yarn tsc --p tsconfig.tests.json",
    "fast-test": "anchor test --skip-build -- --features testing",
    "test": "yarn build:test && yarn fast-test",
    "push": "git push --atomic",
    "pubpush": "yarn build:ts && yarn npm publish && yarn push"
  },
  "dependencies": {
